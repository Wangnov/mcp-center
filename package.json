{
  "name": "mcp-center",
  "version": "0.0.1",
  "description": "MCP Center - 集中管理多个 MCP 服务器的桌面应用",
  "type": "module",
  "scripts": {
    "dev": "npm run export-types && vite",
    "build": "npm run export-types && tsc -b && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "npm run export-types && tauri dev",
    "tauri:build": "npm run export-types && tauri build",
    "export-types": "cd crates/mcp-center && cargo run --bin export-types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write . --ignore-path ./.prettierignore",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
